/*Find online more information about ASCII 
(American Standard Code for Information Interchange) 
and write a program that prints the visible characters 
of the ASCII table on the console 
(characters from 33 to 126 including).
*/
using System;

class PrintTheASCIITable
{
    static void Main(string[] args)
    {
        for (int count = 33; count <= 126; count++)
        {
            char symbol = Convert.ToChar(count);
            Console.Write(symbol);
        }
    }
}

