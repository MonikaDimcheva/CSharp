using System;

class OddOrEven
{
    static void Main(string[] args)
    {
        int number = int.Parse(Console.ReadLine());
        if (number < (-30) || number > 30)
        {
            number = int.Parse(Console.ReadLine());
        }
        if (number % 2 == 0)
        {
            Console.WriteLine("even " + number);
        }
        else {
            Console.WriteLine("odd " + number);
        }
    }
}
